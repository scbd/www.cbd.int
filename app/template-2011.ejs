    <script src="<%=`${cdnUrl}combine/${[
        'npm/jquery@2.2.4/dist/jquery.min.js',
        'npm/jquery-migrate@1.2.1/dist/jquery-migrate.min.js',
        ].concat(baseLibs).concat([
        'npm/popper.js@1.16.0/dist/umd/popper.min.js',
        'npm/bootstrap@3.4.1/dist/js/bootstrap.min.js',
        'npm/requirejs@2.3.6/require.min.js'
    ]).join(',')}`%>"></script>
    <script>
        require.config({ waitSeconds: 30, baseUrl : '/app/', urlArgs: 'v=<%=gitVersion%>' });
        require(["boot"], function(boot){
            var bootFn = boot.default || boot;
            bootFn(window, require, define);
        });
    </script>
    <script src="https://www.recaptcha.net/recaptcha/api.js"></script>
    <script async src='https://www.google-analytics.com/analytics.js'></script>
    <title>Convention on Biological Diversity</title>
    <meta property="og:site_name" content="Convention on Biological Diversity" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="<%=`${cdnUrl}npm/font-awesome@4.7.0/css/font-awesome.css`%>">
    <link rel="stylesheet" href="<%=`${cdnUrl}npm/bootstrap@3.4.1/dist/css/bootstrap.css`%>">
    <link rel="stylesheet" href="<%=`${cdnUrl}npm/animate.css@3.5.2/animate.min.css`%>">
    <style>

    [ng\:cloak], [ng-cloak], .ng-cloak { display: none !important; }

                .mobile-app-visbile,
                .mobile-app-visbile-inline,
    .mobile-app .mobile-app-hidden         { display: none    }
    .mobile-app .mobile-app-visbile        { display: block;  }
    .mobile-app .mobile-app-visbile-inline { display: inline; }

    body, .greybg { background: #f4f4f4; }
    .whitebg      { background: #FFFFFF; }

    header.mobile.navbar              { color:#009B48; background-color:white; margin-bottom: 1px; }
    header.mobile.navbar a            { color:#009B48; }
    header.mobile .navbar-nav a:hover { color:white;background-color:#009B48 }
    header.mobile .navbar-header      { font-weight:semi-bold; }
    header.mobile .navbar-header img.logo {
        height:55px;
        vertical-align:middle;
        margin-left:3px;
    }

    header.large > div.container > div { border-bottom:solid 1px #f4f4f4; }
    header.large   img.logo          { height:70px }
    header.large .navbar-nav a       { color:#009B48; }
    header.large .navbar-nav a:hover { color:white; background-color:#009B48; }


    footer {  display: block; padding: 15px 20px; }
    footer ul { list-style-type: none; padding:0px; }
    footer ul li a { font-size: 14px; color: #666; }
    footer ul li a:hover { color: #000; }
    footer h4 {
      font-size: 14px;
      color: #666;
      font-weight: bold;
    }

    footer.standard ul.list-inline > li { padding: 0px 5px; border-right:solid 1px #666;font-size: 14px;}
    footer.standard ul.list-inline > li:last-child { border-right: none; }

    h1 { font-size: 30px; }
    h2 { font-size: 26px; }
    h3 { font-size: 22px; }
    h4 { font-size: 18px; }
    h5 { font-size: 16px; }
    h6 { font-size: 15px; }

    .view h1, .view h2, .view h3, .view h4, .view h5, .view h6, .view .h1, .view .h2, .view .h3, .view .h4, .view .h5, .view .h6 {
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
      color:#009B48;
    }

    @media (min-width: 768px) {
        button.hidden-xs { display: inline-block !important; }
        span.hidden-xs   { display: inline !important; }
    }

    @media (max-width: 767px) {
        button.visible-xs { display: inline-block !important; }
        span.visible-xs   { display: inline !important; }
    }

    @media print {
      a[href]:after {
        content: none !important;
      }
    }

    </style>
</head>
<body class="ltr bootstrap3" ng-class="::{ 'mobile-app' : templateCtrl.viewOnly }">
    <!-- HEADER -->
    <div ng-if="!templateCtrl.viewOnly">
        <!-- HEADER -->
        <header class="mobile navbar visible-xs" role="banner">
            <div class="container">

                <div class="navbar-header">
                    <button class="navbar-toggle collapsed hidden-print" type="button" data-toggle="collapse" data-target=".navbar-collapse">
                        <i class="fa fa-bars" style="font-size:1.66em;"></i>
                    </button>

                    <div ng-cloak ng-show="user" class="pull-right hidden-print">

                        <a ng-hide="user.isAuthenticated" ng-href="{{templateCtrl.accountsUrl+'/signin?returnUrl='+templateCtrl.getEncodedReturnUrl()}}" class="navbar-toggle collapsed" style="text-decoration:none;color:#009B48" style="color: black">
                            <i  class="fa fa-power-off" style="font-size:1.66em;"></i>
                        </a>

                        <div ng-show="user.isAuthenticated" class="dropdown">

                            <button class="navbar-toggle collapsed" title="{{user.name}}" style="text-decoration:none;color:#009B48" type="button" id="dropdownMenuL" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                            <i class="fa fa-user" style="font-size:1.66em;"></i>
                            </button>

                            <ul class="dropdown-menu" aria-labelledby="dropdownMenuL">
                                <li><a ng-href="{{templateCtrl.accountsUrl+'/profile?returnUrl=' +templateCtrl.getEncodedReturnUrl()}}"><i class="fa fa-user" style="font-size:15px"></i>&nbsp; Profile</a></li>
                                <li><a ng-href="{{templateCtrl.accountsUrl+'/password?returnUrl='+templateCtrl.getEncodedReturnUrl()}}"><i class="fa fa-lock" style="font-size:15px"></i>&nbsp; Password</a></li>
                                <li><a ng-click="templateCtrlsignOut()"  href="/user/signout" ><i class="fa fa-sign-out" style="font-size:15px"></i>&nbsp; Sign-out</a></li>
                            </ul>
                        </div>

                </div> <!--  end sign in-->

                    <a href="/" target="_self">
                        <a href="/"><img class="logo" src="https://attachments.cbd.int/cbd-logo-en.svg?v=<%=gitVersion%>" style="padding-top:4px;padding-left:2px"/></a>
                    </a>

                </div>

                <nav class="collapse navbar-collapse hidden-print" role="navigation">
                    <ul class="nav navbar-nav">
                        <li class="active"><a target="_self" href="/convention">The Convention</a></li>
                        <li class="active"><a target="_self" href="/biosafety">Cartagena Protocol</a></li>
                        <li class="active"><a target="_self" href="/abs">Nagoya Protocol</a></li>
                        <li class="active"><a target="_self" href="/programmes">Programmes</a></li>
                        <li class="active"><a target="_self" href="/information">Information</a></li>
                        <li class="active"><a target="_self" href="/secretariat">Secretariat</a></li>
                    </ul>
                </nav>
            </div>
        </header>

        <!-- LARGE -->
        <header class="large hidden-xs">
            <div class="container whitebg">
                <div>

                    <div class="pull-left" style="margin:5px 0px">
                        <a href="/"><img class="logo" src="https://attachments.cbd.int/cbd-logo-en.svg?v=<%=gitVersion%>"/></a>
                    </div>

                    <div  class="pull-right hidden-print">

                        <a class="btn btn-link" ng-href="{{templateCtrl.accountsUrl+'/signin?returnUrl='+templateCtrl.getEncodedReturnUrl()}}"  style="text-decoration:none;color:#009B48" ng-hide="user.isAuthenticated" style="color: black">
                            <i  class="fa fa-power-off"></i> <span ng-cloak ng-show="user && !user.isAuthenticated">Sign in</span>
                        </a>

                        <div ng-cloak ng-show="user.isAuthenticated" class="dropdown">
                            <button class="btn btn-link dropdown-toggle" style="text-decoration:none;color:#009B48" type="button" id="dropdownMenuL" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                            <span class="caret"></span> {{user.name}}
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenuL">
                                <li><a ng-href="{{templateCtrl.accountsUrl+'/profile?returnUrl=' +templateCtrl.getEncodedReturnUrl()}}"><i class="fa fa-user" style="font-size:15px"></i>&nbsp; Profile</a></li>
                                <li><a ng-href="{{templateCtrl.accountsUrl+'/password?returnUrl='+templateCtrl.getEncodedReturnUrl()}}"><i class="fa fa-lock" style="font-size:15px"></i>&nbsp; Password</a></li>
                                <li><a ng-click="templateCtrl.signOut()"  href="/user/signout" ><i class="fa fa-sign-out" style="font-size:15px"></i>&nbsp; Sign-out</a></li>
                            </ul>
                        </div>

                </div> <!--  end sign in-->

                    <div style="clear:right;" class="pull-right hidden-print">
                        <ul class="nav navbar-nav">
                            <li class="active"><a href="/convention">The Convention</a></li>
                            <li class="active"><a href="/biosafety">Cartagena Protocol</a></li>
                            <li class="active"><a href="/abs">Nagoya Protocol</a></li>
                            <li class="active"><a href="/programmes">Programmes</a></li>
                            <li class="active"><a href="/information">Information</a></li>
                            <li class="active hidden-sm"><a href="/secretariat">Secretariat</a></li>
                        </ul>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </header>

        <div id="conferenceHeader" style="display:none;"></div>
    </div>

    <!-- CONTENT -->
    <a name="view"></a>
    <div class="container whitebg view" >
        <ng-view></ng-view>
    </div>

    <!-- FOOTER -->
    <div ng-if="!templateCtrl.viewOnly">
      <footer class="container whitebg hidden-print">
        <div class="greybg">
            <div class="col-sm-4 fleft">
                <h4>Quick Links</h4>
                <ul>
                    <li><a target="_self" href="/decisions">Decisions</a></li>
                    <li><a target="_self" href="/meetings">Meetings</a></li>
                    <li><a target="_self" href="/notifications">Notifications</a></li>
                    <li><a target="_self" href="/news">News Headlines</a></li>
                    <li><a target="_self" href="/publications">Publications</a></li>
                    <li><a target="_self" href="/chm">CHM</a></li>
                    <li><a target="_self" href="http://bch.cbd.int/">BCH</a></li>
                </ul>
            </div>
            <div class="col-sm-4 fleft">
                <h4 class="hidden-xs">&nbsp;</h4>
                <ul>
                    <li><a target="_self" href="/parties">Parties</a></li>
                    <li><a target="_self" href="/countries">Country Profiles</a></li>
                    <li><a target="_self" href="/nfp">National Focal Point</a></li>
                    <li><a target="_self" href="/nbsap">National Strategies (NBSAPs)</a></li>
                    <li><a target="_self" href="/reports">National Reports</a></li>
                    <li><a target="_self" href="/contributions">Contributions</a></li>
                    <li><a target="_self" href="/cooperation">Cooperation & Partnerships</a></li>
                </ul>
            </div>
            <div class="col-sm-4 fleft">
                <h4>Follow us</h4>
                <ul>
                  <li><img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/ic-rss.png">&nbsp;&nbsp;<a href="https://www.cbd.int/rss">RSS</a></li>
                  <li><img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/ic-twitter.png">&nbsp;&nbsp;<a href="https://twitter.com/unbiodiversity">Twitter</a></li>
                  <li><img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/ic-facebook.png">&nbsp;&nbsp;<a href="https://www.facebook.com/UNBiodiversity">Facebook</a></li>
                  <li><img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/ic-youtube.png">&nbsp;&nbsp;<a href="https://www.youtube.com/user/chmcbd">YouTube</a></li>
                  <li><img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/ic-instagram.png">&nbsp;&nbsp;<a href="https://instagram.com/unbiodiversity">Instagram</a></li>
                  <li><img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/ic-linkedin.png">&nbsp;&nbsp;<a href="https://www.linkedin.com/company/unbiodiversity">LinkedIn</a></li>
                </ul>

            </div>

            <div class="clearfix"></div>

        </div>
      </footer>

      <footer class="container standard whitebg">
          <div class="col-md-9 hidden-print">
              <ul class="list-inline">
                  <li><a target="_self" href="/feedback">Feedback</a></li>
                  <li><a target="_self" href="/map">Site Map</a></li>
                  <li><a target="_self" href="/contact">Contact Us</a></li>
                  <li><a target="_self" href="/terms">Terms of Use</a></li>
                  <li><a target="_self" href="/privacy">Privacy Policy</a></li>
                  <li><a target="_self" href="/credits">Credits</a></li>
                  <li><a target="_self" href="/copyright">&copy; CBD Secretariat</a></li>
              </ul>
          </div>
          <div class="col-md-3 pull-right">
              <a href="http://www.un.org/" target="_blank"><img src="/app/images/un-logo-white.gif?v=<%=gitVersion%>" alt="UN logo" /></a>
              <a href="http://unep.org/"   target="_blank"><img src="/app/images/unep-logo-white-en.png?v=<%=gitVersion%>" alt="UNEP logo" /></a>
          </div>
      </footer>
    </div>

    <!-- JS screen-size detector -->
    <div class="device-size visible-xs" size="xs"></div>
    <div class="device-size visible-sm" size="sm"></div>
    <div class="device-size visible-md" size="md"></div>
    <div class="device-size visible-lg" size="lg"></div>
</body>
</html>
